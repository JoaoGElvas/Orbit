{
  "name": "orbit-task-manager",
  "version": "1.0.0",
  "description": "Organizador de tarefas pessoal com tema de viagem espacial",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run db:migrate",
    "railway:deploy": "npm run db:migrate && npm start",
    "setup-db": "node scripts/setup-database.js",
    "security-check": "./scripts/security-check.sh",
    "audit": "npm audit --audit-level=moderate",
    "audit-fix": "npm audit fix",
    "db:migrate": "knex migrate:latest",
    "db:migrate:rollback": "knex migrate:rollback --batch=1",
    "db:migrate:rollback:all": "knex migrate:rollback --all",
    "db:migrate:status": "knex migrate:status",
    "db:migrate:make": "knex migrate:make",
    "db:seed": "knex seed:run",
    "db:seed:make": "knex seed:make",
    "db:reset": "npm run db:migrate:rollback:all && npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "task-manager",
    "space-theme",
    "productivity"
  ],
  "author": "Jo√£o Elvas",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "knex-cli": "^1.0.1",
    "nodemon": "^3.0.1"
  }
}
